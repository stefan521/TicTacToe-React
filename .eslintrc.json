{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "jest": true,
        "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
        "import/extensions": ["error", {
            "jsx": "always"
          }]
    }
}